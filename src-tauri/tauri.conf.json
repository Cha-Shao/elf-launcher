{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "elf-launcher",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true,
        "execute": true,
        "scope": [
          {
            "name": "cmd",
            "cmd": "cmd",
            "args": true
          }
        ]
      },
      "window": {
        "close": true,
        "minimize": true,
        "setMaximizable": true,
        "startDragging": true
      },
      "fs": {
        "all": true,
        "scope": [
          "$RESOURCE/EMCL",
          "$RESOURCE/EMCL/**",
          "$RESOURCE/.minecraft",
          "$RESOURCE/.minecraft/**",
          "$DATA/.minecraft",
          "$DATA/.minecraft/**"
        ]
      },
      "path": {
        "all": true
      },
      "dialog": {
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.tauri.dev",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Elf启动器",
        "width": 1280,
        "minWidth": 1280,
        "height": 800,
        "minHeight": 800,
        "transparent": true,
        "decorations": false
      }
    ]
  }
}